{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend Desafio Fullstack",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "test:unit": "jest",
    "test:functional": "jest --projects ./test --runInBand",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "db:migration:generate": "yarn run typeorm migration:generate -n",
    "db:migration:revert": "yarn run typeorm migration:revert",
    "db:migrate": "yarn run typeorm migration:run",
    "db:entity:create": "yarn run typeorm entity:create -n",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.10.2",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "jest": "^26.4.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "pg": "^8.3.3",
    "typeorm": "^0.2.26"
  }
}